{
  "name": "git-ex",
  "version": "0.1.0",
  "description": "git-ex is git extensions tool",
  "type": "module",
  "scripts": {
    "start": "node ./src/index.js",
    "localInstall": "npm install -g ./",
    "format": "prettier --write 'src/**/*.js'",
    "checkPrettier": "prettier --check 'src/**/*.js'",
    "uninstall": "npm uninstall -g git-ex"
  },
  "bin": {
    "git-ex": "./src/index.js"
  },
  "author": "yasukotelin",
  "license": "ISC",
  "devDependencies": {
    "prettier": "^2.5.1"
  },
  "dependencies": {
    "commander": "^8.3.0",
    "prompts": "^2.4.2"
  }
}